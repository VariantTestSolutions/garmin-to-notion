
# Notion
NOTION_TOKEN=secret_notion_token
# EITHER set your DB id OR a parent page id (if you want the script to create the DB)
NOTION_DB_ID=
NOTION_PARENT_PAGE_ID=

# Garmin credentials (prefer using tokens + cache; only set email/pass if needed)
GARMIN_EMAIL=your@email
GARMIN_PASSWORD=your_password
# Optional: path to token cache (script defaults to ~/.garmin_tokens)
GARMIN_TOKEN_STORE=~/.garmin_tokens
# Optional: only if you have a fixed MFA code (most MFA codes are one-time and not suitable for CI)
GARMIN_MFA_CODE=
